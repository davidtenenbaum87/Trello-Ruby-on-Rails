<ul class="container">
  <h3> Project Index </h3>
  <% @projects.each do |project| %>
    <li>
      <div class="card blue lighten-2">
        <div class="card-content white-text">
          <span class="card-title"> <%= project.name.capitalize %> </span>
          <div class="card-action">
            <%= link_to "View", project%>
          </div>
        </div>
      </div>
    </li>
  <% end %>
</ul>
