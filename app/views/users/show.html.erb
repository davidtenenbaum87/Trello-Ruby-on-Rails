<%= @user.name %>
<%= @user.email %>
<%= @user.created_at.strftime("%m/%d/%Y") %>
<%= "picture" %>
<ol>
  <% @projects.uniq.each do |project| %>
    <li><%= link_to project.name, project %></li>
      <ol>
      <% project.tasks.each do |task| %>
        <% if task.user_id == session[:user_id] %>
          <li><%= link_to task.title, task %></li>
        <% end%>
      <% end %>
      </ol>
  <% end %>
</ol>